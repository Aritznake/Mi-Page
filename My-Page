    <script>
        function ocultarOtrosDivs(triggeringDiv) {
            var divsOcultable = 
            document.querySelectorAll('.div-ocultable');
            divsOcultable.forEach(function(div) {
                if (div !== triggeringDiv) {
                    div.style.display = 0;
                }
            });
        } 
    </script>
